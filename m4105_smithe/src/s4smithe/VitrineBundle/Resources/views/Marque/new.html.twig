{% set namePage = "Marque | Création" %}
{% set typePage = namePage %}
{% extends "s4smitheVitrineBundle::layoutBackOffice.html.twig" %}

{% block linkFollowing %}
	<li class="active" >Marque</li >
	<li >Création</li >
{% endblock %}

{% block content %}
	<div class="col-xs-12" >
		{{ form_start(form) }}
		{{ form_widget(form) }}
		<button type="reset" title="Annuler" class="btn btn-sm btn-default" >Annuler</button >
		<button type="submit" title="Créer" class="btn btn-sm btn-primary" >Créer</button >
		{{ form_end(form) }}

		<a href="{{ path('marque_index') }}" title="Retour à la liste" class="btn btn-sm btn-default" >Retour</a >
	</div >
{% endblock %}
